{
    "name": "rusty",
    "displayName": "Rusty Theme",
    "publisher": "chrisnevers",
    "repository": {
        "url": "http://github.com/chrisnevers/rusty-theme"
    },
    "description": "A simple, soft color theme",
    "version": "0.4.0",
    "engines": {
        "vscode": "^1.42.0"
    },
    "icon": "images/icon.png",
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Rusty",
                "uiTheme": "vs",
                "path": "./themes/Rusty-color-theme.json"
            }
        ],
        "grammars": [
            {
                "path": "./syntaxes/inject.json",
                "scopeName": "todo-comment.injection",
                "injectTo": [
                    "source.ocaml",
                    "source.java",
                    "source.haskell"
                ]
            }
        ]
    }
}
